{
  "expo": {
    "name": "Giacomelli App",
    "slug": "mototaxi-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/loc.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logomototaxi.png",
      "resizeMode": "contain",
      "backgroundColor": "yellow"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logomototaxi.png",
        "backgroundColor": "yellow"
      },
      "package": "com.smithgg.mototaxiapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logomototaxi.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5ee9caa5-9d48-4d22-97a0-f92e6a77059a"
      }
    }
  }
}
